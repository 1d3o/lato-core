<div class="widgets-systeminfo">

  <%=raw cell(:elements, :title).new(label: 'Lato Modules', size: 6) %>

  <% lato_modules = core__get_modules_list %>
  <% lato_modules.each do |module_name| %>
    <span class="widgets-systeminfo__module-name"><%= module_name %></span>
  <% end %>

  <p class="widgets-systeminfo__footer">
    Lato is developed with <i class="fa fa-heart" aria-hidden="true"></i> by Ideo Creative Network.
  </p>

</div>