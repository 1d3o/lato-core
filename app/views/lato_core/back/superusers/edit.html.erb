<%
  row = cell(:elements, :row).new
  block = cell(:elements, :block).new(class: 'xs-12 sm-12 md-12')
%>

<!-- MAIN CONTAINER -->
<%=raw row.open %>

  <%=raw block.open %>

    <%= render 'lato_core/back/superusers/shared/form', url: lato_core.superuser_path(@superuser.id), method: :put %>

  <%=raw block.close %>

<%=raw row.close %>
<!-- / MAIN CONTAINER -->